# Server Configuration
NODE_ENV=development
PORT=3001

# Frontend URL for CORS
FRONTEND_URL=http://localhost:5173

# JWT Secret for authentication
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Session Secret
SESSION_SECRET=your-session-secret-change-this-too

# Database Configuration (SQLite)
DB_PATH=./database.sqlite

# Upload Configuration
UPLOAD_PATH=./uploads
MAX_FILE_SIZE=10485760

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100
PREDICTION_RATE_LIMIT_WINDOW_MS=60000
PREDICTION_RATE_LIMIT_MAX=10

# TensorFlow.js Model Paths (Optional - defaults to relative paths)
# TFJS_MODEL_PATH=/absolute/path/to/klasifikasi-tanaman/tfjs_model/model.json
# TFJS_CLASSES_PATH=/absolute/path/to/klasifikasi-tanaman/tfjs_model/classes.json

# Example for different locations:
# If moved to separate project:
# TFJS_MODEL_PATH=D:/Projects/klasifikasi-tanaman/tfjs_model/model.json
# TFJS_CLASSES_PATH=D:/Projects/klasifikasi-tanaman/tfjs_model/classes.json